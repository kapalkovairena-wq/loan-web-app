{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "/node_modules/"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "https://www.kreditsch.de",
        "type": 301
      },
      {
        "source": "/**",
        "destination": "https://www.kreditsch.de",
        "type": 301
      }
    ]
  }
}